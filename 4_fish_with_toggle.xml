                </actionsequence>
            </actionsequenceondevice>
        </onswitchactivate>
        <onswitchdeactivate>
            <actionsequenceondevice>
                <friendlyname>Default</friendlyname>
                <actionsequence>
                    <turnswitchesoff>2</turnswitchesoff>
