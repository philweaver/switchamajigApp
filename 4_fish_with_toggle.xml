        <rgbacolor>1.0 1.0 0.0 1.0</rgbacolor>
        <switchtext>Tilt Down: 4</switchtext>
        <onswitchactivate>
            <actionsequenceondevice>
                <friendlyname>Default</friendlyname>
                <actionsequence>
                    <turnswitcheson>4</turnswitcheson>
                </actionsequence>
            </actionsequenceondevice>
        </onswitchactivate>
