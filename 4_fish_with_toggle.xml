    <panelelement>
        <frame>50 495 944 173</frame>
        <rgbacolor>1.0 0.7 0.0 1.0</rgbacolor>
        <switchtext>Swim: Toggle 1 and 2 for 1/2 second each</switchtext>
        <onswitchactivate>
            <actionsequenceondevice>
                <loop>
                    <friendlyname>Default</friendlyname>
                    <turnSwitchesOn>1</turnSwitchesOn>
                    <delay>0.5</delay>
                    <turnSwitchesOff>1</turnSwitchesOff>
                    <turnSwitchesOn>2</turnSwitchesOn>
                    <delay>0.5</delay>
                    <turnSwitchesOff>2</turnSwitchesOff>
                </loop>
            </actionsequenceondevice>
        </onswitchactivate>
        <onswitchdeactivate>
            <stoploop/>
            <actionsequenceondevice>
                <friendlyname>Default</friendlyname>
                <actionsequence>
                    <turnSwitchesOff>1 2</turnSwitchesOff>
                </actionsequence>
            </actionsequenceondevice>
        </onswitchdeactivate>
