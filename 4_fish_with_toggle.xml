    <panelelement>
        <frame>50 50 192 395</frame>
        <rgbacolor>0.0 1.0 0.0 1.0</rgbacolor>
        <switchtext>Tail Left: 1</switchtext>
        <onswitchactivate>
            <actionsequenceondevice>
                <friendlyname>Default</friendlyname>
                <actionsequence>
                    <turnswitcheson>1</turnswitcheson>
                </actionsequence>
            </actionsequenceondevice>
        </onswitchactivate>
        <onswitchdeactivate>
            <actionsequenceondevice>
                <friendlyname>Default</friendlyname>
                <actionsequence>
                    <turnswitchesoff>1</turnswitchesoff>
